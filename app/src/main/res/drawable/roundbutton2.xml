<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <stroke android:width="0.1dip" android:color="#1d000000" />
            <gradient android:angle="-90" android:startColor="#1d000000" android:endColor="#689a92"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <stroke android:width="0.1dip" android:color="#5e7974" />
            <solid android:color="#1d000000"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <stroke android:width="0.1dip" android:color="#00000000" />
            <gradient android:angle="-90" android:startColor="#be000000" android:endColor="#000000" />
        </shape>
    </item>
</selector>