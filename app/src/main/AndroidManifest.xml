<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.bisch.csgo_smokes">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/cloud91"
        android:label="SmokeGod: Mirage"
        android:roundIcon="@drawable/cloud91"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mirage.mirage"
            android:label="MIRAGE"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".dust2.dust2"
            android:label="@string/dust2"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".cache.cache"
            android:label="@string/cache"
            android:screenOrientation="portrait"/>
        <activity android:name=".mirage.mirage_sitea_stairs.mirage_sitea_stairs"
            android:label="A: Stairs"
            android:screenOrientation="portrait"
            />
        <activity android:name=".mirage.mirage_sitea_con_jun.mirage_sitea_con_jun"
            android:label="A: Connector &amp; Jungle"
            android:screenOrientation="portrait"/>
        <activity android:name=".mirage.mirage_sitea_ct.mirage_sitea_ct"
            android:label="A: CT-Spawn"
            android:screenOrientation="portrait"/>
        <activity android:name=".mirage.mirage_mid_cat.mirage_mid_cat"
            android:label="Mid: Catwalk/Short "
            android:screenOrientation="portrait"/>
        <activity android:name=".mirage.mirage_mid_window_t_spawn.mirage_mid_window_t_spawn"
            android:label="Mid: Window (T-Spawn)"
            android:screenOrientation="portrait"/>
        <activity android:name=".mirage.mirage_mid_window_b_apps.mirage_mid_window_b_apps"
            android:label="Mid: Window (B Appartments)"
            android:screenOrientation="portrait"/>
        <activity android:name=".mirage.mirage_mid_connector_b_apps.mirage_mid_connector_b_apps"
            android:label="Mid: Connector (B Appartments)"
            android:screenOrientation="portrait"/>
        <activity android:name=".mirage.mirage_siteb_bench.mirage_siteb_bench"
            android:label="B: Bench"
            android:screenOrientation="portrait"/>
        <activity android:name=".mirage.mirage_siteb_under_balc.mirage_siteb_under_balc"
            android:label="B: Under Balcony"
            android:screenOrientation="portrait"/>
        <activity android:name=".mirage.mirage_siteb_market_window.mirage_siteb_market_window"
            android:label="B: Market window"
            android:screenOrientation="portrait"/>
        <activity android:name=".mirage.mirage_siteb_short_cat.mirage_siteb_short_cat"
            android:label="B: Short"
            android:screenOrientation="portrait"/>
    </application>

</manifest>